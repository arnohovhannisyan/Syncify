{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Prepare",
			"type": "shell",
			"command": "node ${workspaceFolder}/scripts/prepare.js"
		},
		{
			"label": "Start",
			"type": "shell",
			"command": "node",
			"args": ["${workspaceFolder}/scripts/run.js", "${execPath}"],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"dependsOn": ["Prepare"]
		}
	]
}
