{
  "name": "syncify",
  "displayName": "Syncify",
  "description": "Sync your settings and extensions across multiple machines",
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/main.js",
  "contributes": {
    "commands": [
      {
        "command": "syncify.sync",
        "title": "%cmd(syncify.sync).name%"
      },
      {
        "command": "syncify.upload",
        "title": "%cmd(syncify.upload).name%"
      },
      {
        "command": "syncify.download",
        "title": "%cmd(syncify.download).name%"
      },
      {
        "command": "syncify.reset",
        "title": "%cmd(syncify.reset).name%"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --config .webpack/production.js",
    "compile": "webpack --config .webpack/development.js",
    "watch": "webpack --config .webpack/development.js --watch",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.12.21",
    "ts-loader": "^6.0.4",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3",
    "vscode": "^1.1.35",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6"
  }
}
